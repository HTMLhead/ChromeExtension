{
    "manifest_version": 2,
   
    "name": "허리를 위하여",
    "description": "스트레칭 타이머",
    "version": "1.0",

    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },

    "browser_action": {
      "default_icon": "IHurtMyBack.png",
      "default_popup": "IHurtMyBack.html"
    },
    
    "permissions": ["alarms", "notifications", "storage"]
  }

  