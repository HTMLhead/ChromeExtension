{
    "manifest_version": 2,
   
    "name": "허리를 위하여",
    "description": "스트레칭 타이머",
    "version": "1.3",
    "permissions": ["alarms", "notifications", "storage"],

    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },

    "browser_action": {
      "default_icon": "./extensionImage/IHurtMyBack.png",
      "default_popup": "IHurtMyBack.html"
    }
  }

  